        "BESPOKE_WITH_LINKER": "true"
      },
      "log_date_format": "YYYY-MM-DD HH:mm Z"
    },
    {
      "script": "../game/Proxy/Qualtrics/build/serve.js",
      "name": "Qualtrics",
      "env": {
        "NODE_ENV": "production",
        "BESPOKE_WITH_PROXY": "true",
        "BESPOKE_WITH_LINKER": "true"
      },
      "log_date_format": "YYYY-MM-DD HH:mm Z"
