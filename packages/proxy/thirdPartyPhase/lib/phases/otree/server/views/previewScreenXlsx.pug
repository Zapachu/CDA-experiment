html
    head
        title= 'ExportScreen'
        style.
            .screenTable {
                border-collapse: collapse;
            }

            .screenTable th,td {
                padding: .5rem 1.5rem;
                text-align: center;
            }
    body
        a(href=`/downloadScreenXlsx/${phaseId}`) Download
        table(class='screenTable', border='1')
            tbody
                tr
                    th Player
                    th ScreenWidth(px)
                    th ScreenHeight(px)
                    th IP Adress
                    th User Agent
                    th User System
                    th User System Version
                each screen in screens
                    tr
                        td=screen.hash
                        td=screen.winW
                        td=screen.winH
                        td=screen.ipAddress
                        td=screen.userAgent
                        td=screen.userSystem
                        td=screen.userSystemVersion
