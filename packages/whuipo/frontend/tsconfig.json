{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "esnext",
      "dom",
      "scripthost"
    ],
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "downlevelIteration": true,
    "jsx": "react",
    "noEmit": true
  }
}
