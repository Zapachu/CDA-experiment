{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es5",
      // "strict": true,
      "noUnusedLocals": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "lib": ["es7", "es2017.object"],
      "outDir": "build",
      "baseUrl": ".",
      "paths": {
        "*": ["v5/types/*", "node_modules/*"]
      },
      "experimentalDecorators": true,
      "skipLibCheck": true
    },
    "exclude": ["./dist", "./frontend"]
  }
  